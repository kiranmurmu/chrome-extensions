{
  "name": "getting-started",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export -o build",
    "babel": "babel scripts --out-dir build --extensions .ts",
    "rename": "mv build/_next build/public",
    "replace": "sed -i 's/\\/_next/\\/public/g' build/**.html",
    "postbuild": "npm run export",
    "postexport": "npm run babel",
    "postbabel": "npm run rename",
    "postrename": "npm run replace"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.7",
    "@babel/preset-typescript": "7.16.7",
    "@types/chrome": "0.0.180",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@types/uuid": "8.3.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "next-build-id": "3.0.0",
    "typescript": "4.6.2",
    "uuid": "8.3.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  }
}
